  "repository": {
    "url": "git+https://github.com/dimaslz/ng-heroicons.git",
    "type": "git"
  },
  "private": true,
  "scripts": {
    "generate": "node ./generator/build-components.js",
    "test": "jest",

    "--------- Angular v12 Commands ---------": "",
    "v12-copy-playground-app": "cpy assets/v12/app.component.html packages/angular-v12/projects/playground/src/app --flat",
    "v12-copy-solid-icon-components": "cpy assets/components/solid packages/angular-v12/projects/lib/components/solid --flat",
    "v12-copy-outline-icon-components": "cpy assets/components/outline packages/angular-v12/projects/lib/components/outline --flat",
    "v12-copy-component-variables": "cpy assets/variables packages/angular-v12/projects/lib/variables --flat",
    "v12-watch:lib": "yarn --cwd=packages/angular-v12/projects build lib --watch --configuration production",
    "v12-copy-assets": "yarn v12-copy-solid-icon-components && yarn v12-copy-outline-icon-components && yarn v12-copy-component-variables && yarn v12-copy-playground-app",
    "v12-preinstall": "yarn v12-copy-assets",
    "v12-start:playground": "yarn --cwd=packages/angular-v12/projects start playground",
    "--------- ! Angular v12 Commands ---------": "",

    "echo": "echo 'MAIN ECHO'"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "camelcase": "^6.3.0",
    "cpy-cli": "^4.2.0",
    "dedent": "^0.7.0",
    "jest": "^29.2.2",
    "jest-cli": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-preset-angular": "^12.2.2",
    "lodash.kebabcase": "^4.1.1",
    "mkdirp": "^1.0.4",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "svgo": "^3.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1"
  },
  "dependencies": {}
}
