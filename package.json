{
  "name": "@dimaslz/ng-heroicons",
  "version": "1.0.0",
  "author": "Dimas LÃ³pez <me@dimaslz.io> (dimaslz)",
  "repository": {
    "url": "git+https://github.com/dimaslz/ng-heroicons.git",
    "type": "git"
  },
  "private": true,
  "scripts": {
    "--------- Angular v11 Commands ---------": "",
    "v11-copy-common-components": "cpy assets/components/common packages/angular-v11/projects/lib/components/common --flat",
    "v11-copy-outline-components": "cpy assets/components/outline packages/angular-v11/projects/lib/components/outline --flat",
    "v11-copy-solid-components": "cpy assets/components/solid packages/angular-v11/projects/lib/components/solid --flat",
    "v11-copy-playground-app": "cpy assets/v11/app.component.html packages/angular-v11/projects/playground/src/app --flat",
    "v11-watch:lib": "yarn --cwd=packages/angular-v11/projects build lib --watch --configuration production",
    "v11-copy-assets": "yarn v11-copy-common-components && yarn v11-copy-outline-components && yarn v11-copy-solid-components && yarn v11-copy-playground-app",
    "build:v11": "yarn --cwd=packages/angular-v11 build lib --configuration production",
    "clean:v11": "rimraf packages/angular-v11/dist && rimraf packages/angular-v11/projects/lib/components",
    "update:v11": "yarn clean:v11 && yarn v11-copy-assets",
    "reinstall:v11": "rimraf packages/angular-v11/node_modules && yarn --cwd=packages/angular-v11 install",
    "v11-copy-release": "rimraf dist/v11 && mkdirp dist/v11 && cp -Rf packages/angular-v11/dist/lib/* dist/v11",
    "--------- ! Angular v11 Commands ---------": "",
    "--------- Angular v12 Commands ---------": "",
    "v12-copy-common-components": "cpy assets/components/common packages/angular-v12/projects/lib/components/common --flat",
    "v12-copy-outline-components": "cpy assets/components/outline packages/angular-v12/projects/lib/components/outline --flat",
    "v12-copy-solid-components": "cpy assets/components/solid packages/angular-v12/projects/lib/components/solid --flat",
    "v12-copy-playground-app": "cpy assets/v12/app.component.html packages/angular-v12/projects/playground/src/app --flat",
    "v12-watch:lib": "yarn --cwd=packages/angular-v12/projects build lib --watch --configuration production",
    "v12-copy-assets": "yarn v12-copy-common-components && yarn v12-copy-outline-components && yarn v12-copy-solid-components && yarn v12-copy-playground-app",
    "build:v12": "yarn --cwd=packages/angular-v12 build lib --configuration production",
    "clean:v12": "rimraf packages/angular-v12/dist && rimraf packages/angular-v12/projects/lib/components",
    "update:v12": "yarn clean:v12 && yarn v12-copy-assets",
    "reinstall:v12": "rimraf packages/angular-v12/node_modules && yarn --cwd=packages/angular-v12 install",
    "v12-copy-release": "rimraf dist/v12 && mkdirp dist/v12 && cp -Rf packages/angular-v12/dist/lib/* dist/v12",
    "--------- ! Angular v12 Commands ---------": "",
    "--------- Angular v13 Commands ---------": "",
    "v13-copy-common-components": "cpy assets/components/common packages/angular-v13/projects/lib/components/common --flat",
    "v13-copy-outline-components": "cpy assets/components/outline packages/angular-v13/projects/lib/components/outline --flat",
    "v13-copy-solid-components": "cpy assets/components/solid packages/angular-v13/projects/lib/components/solid --flat",
    "v13-copy-playground-app": "cpy assets/v13/app.component.html packages/angular-v13/projects/playground/src/app --flat",
    "v13-watch:lib": "yarn --cwd=packages/angular-v13/projects build lib --watch --configuration production",
    "v13-copy-assets": "yarn v13-copy-common-components && yarn v13-copy-outline-components && yarn v13-copy-solid-components && yarn v13-copy-playground-app",
    "build:v13": "yarn --cwd=packages/angular-v13 build lib --configuration production",
    "clean:v13": "rimraf packages/angular-v13/dist && rimraf packages/angular-v13/projects/lib/components",
    "update:v13": "yarn clean:v13 && yarn v13-copy-assets",
    "reinstall:v13": "rimraf packages/angular-v13/node_modules && yarn --cwd=packages/angular-v13 install",
    "v13-copy-release": "rimraf dist/v13 && mkdirp dist/v13 && cp -Rf packages/angular-v13/dist/lib/* dist/v13",
    "-------- ! Angular v13 Commands --------": "",
    "---------- Angular v14 Commands ----------": "",
    "v14-copy-common-components": "cpy assets/components/common packages/angular-v14/projects/lib/components/common --flat",
    "v14-copy-outline-components": "cpy assets/components/outline packages/angular-v14/projects/lib/components/outline --flat",
    "v14-copy-solid-components": "cpy assets/components/solid packages/angular-v14/projects/lib/components/solid --flat",
    "v14-copy-playground-app": "cpy assets/v14/app.component.html packages/angular-v14/projects/playground/src/app --flat",
    "v14-watch:lib": "yarn --cwd=packages/angular-v14/projects build lib --watch --configuration production",
    "v14-copy-assets": "yarn v14-copy-common-components && yarn v14-copy-outline-components && yarn v14-copy-solid-components && yarn v14-copy-playground-app",
    "build:v14": "yarn --cwd=packages/angular-v14 build lib --configuration production",
    "clean:v14": "rimraf packages/angular-v14/dist && rimraf packages/angular-v14/projects/lib/components",
    "update:v14": "yarn clean:v14 && yarn v14-copy-assets",
    "reinstall:v14": "rimraf packages/angular-v14/node_modules && yarn --cwd=packages/angular-v14 install",
    "v14-copy-release": "rimraf dist/v14 && mkdirp dist/v14 && cp -Rf packages/angular-v14/dist/lib/* dist/v14",
    "--------- ! Angular v14 Commands ---------": "",
    "------------ General Commands ------------": "",
    "generate": "node generator/index.mjs",
    "test": "jest",
    "clean-dist": "rimraf clean/v11 && rimraf dist/v12 && rimraf dist/v13 && rimraf dist/v14",
    "clean-projects": "yarn clean:v11 && yarn clean:v12 && yarn clean:v13 && yarn clean:v14",
    "update-projects": "yarn update:v11 && yarn update:v12 && yarn update:v13 && yarn update:v14",
    "build:prod": "yarn build:v11 && yarn build:v12 && yarn build:v13 && yarn build:v14",
    "copy-releases": "yarn v11-copy-release && yarn v12-copy-release && yarn v13-copy-release && yarn v14-copy-release",
    "release": "node release/index.mjs",
    "----------- ! General Commands -----------": ""
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "axios": "^1.1.3",
    "camelcase": "^6.3.0",
    "chalk": "^5.1.2",
    "command-line-args": "^5.2.1",
    "cpy-cli": "^4.2.0",
    "dedent": "^0.7.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "jest": "^29.2.2",
    "jest-cli": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-preset-angular": "^12.2.2",
    "lodash.kebabcase": "^4.1.1",
    "mkdirp": "^1.0.4",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
